{"version":2,
"builds":[
{
    "src":"index.js",
    "use":"@vercel/node"
    },{
        "src":"public/*",
        "use":"@vercel/static"
    },{
        "src":"controllers/*.js",
        "use":"@vercel/node"
    },{
        "src":"models/*.js",
        "use":"@vercel/node"
    }

],
"routes":[
    {
        "src":"/routers/(.*)",
        "dest":"/routers/$1"
    }, {
        "src":"/(.*)",
        "dest":"/public/$1"
    }
]}
